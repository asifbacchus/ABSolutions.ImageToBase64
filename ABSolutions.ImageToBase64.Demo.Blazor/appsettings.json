{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ABSolutions.ImageToBase64": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Base64Converter": {
    "UpstreamImageAssetBaseUri": "https://picsum.photos/",
    "UpstreamImageRetrievalTimeoutSeconds": 5,
    "EnableBase64Cache": true,
    "NoExpiry": false,
    "Base64CacheExpiryMinutes": 5
  }
}
